{
    "flex-layout.row#desktop__footer--comprasegura": {
        "title": "desktop__footer--comprasegura",
        "children": [
            "header-row#footer__comprasegura--container"
        ],
        "props": {
            "blockClass": "desktop__footer--comprasegura"
        }
    },
    "header-row#footer__comprasegura--container":{
        "title":"footer__comprasegura--container",
        "children": [
            "rich-text#comprasegura__description--one",
            "image#comprasegura__logo--certificado",
            "image#comprasegura__logo--criptografia",
            "rich-text#comprasegura__description--two",
            "image#comprasegura__logo--blindado",
            "rich-text#comprasegura__description--three"
